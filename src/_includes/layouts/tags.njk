---
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /tags/{{ tag }}/
layout: layouts/base.njk
---
  <div class="content">
<h1>{{ tag }}</h1>

<ul>
{% set taglist = collections[ tag ] %}
{% for post in taglist | reverse %}
          {% include './listed-post.njk' %}
{% endfor %}
</ul>
</div>
